<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>

    <class name="roster.Roster" table="gt_roster">
    	<id name="uuid" type="string" column="uuid">
    		<generator class="uuid.hex"></generator>
    	</id>
    	<property name="htNumber" column="htNumber"/>
    	<property name="name" column="name"/>
    	<property name="sex" column="sex"/>
    	<property name="identityId" column="identityId"/>
    	<property name="bankCard" column="bankCard"/>
    	<property name="sbNumber" column="sbNumber"/>
    	<property name="address" column="address"/>
    	<property name="htStart"  column="htStart"/>
    	<property name="htEnd"  column="htEnd"/>
    	<property name="jobType" column="jobType"/>
    	<property name="phone" column="phone"/>
    	<property name="lcTime" column="lcTime"/>
    	<property name="sbStart" column="sbStart"/>
    	<property name="sbEnd" column="sbEnd"/> 
    	<property name="sbRemind"  column="sbRemind"/>
    	<property name="bxType" column="bxType"/>
    	<property name="beizhu"  column="beizhu"/>
    	<property name="del" column="del"/>
    	<property name="gsqk" column="gsqk"/>
    	<property name="gsStart" column="gsStart"/>
    	<property name="gsEnd" column="gsEnd"/>
    	<property name="createDate" column= "createDate"/> 
    	<property name="source" column="source"/>
    	<property name="scz" column= "scz"/>
    	<property name="addinkq" column= "addinkq"/>
    	<property name="image" length="1000" column= "image" />
		<property name="deal" column="deal"/>

		<property name="createDate2" type="timestamp" column= "createDate2"/>
	</class>
</hibernate-mapping>