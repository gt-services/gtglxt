<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>

    <class name="px.PxInfo" table="gt_pxinfo">
    	<id name="uuid" type="string" column="uuid">
    		<generator class="uuid.hex"></generator>
    	</id>
    	<property name="signupDate" type="string" column="signupdate"/>
    	<property name="name" column="name"/>
    	<property name="sex" column="sex" length="10"/>
    	<property name="idCard" column="idcard"/>
    	<property name="levelEdu" column="leveledu"/>
    	<property name="unitOrPerson" column="unitorperson" length="500"/>
    	<property name="telephone" column="telephone"/>
    	<property name="contactOfUnit" column="contactofunit"/>
    	<property name="unitPhone" column="unitphone"/>
    	<property name="trainPlace" column="trainplace"/>
    	<property name="trainType" column="traintype"/>
    	<property name="test" column="test"/>
    	<property name="standardAmount" column="standardamount"/>
    	<property name="discountAmount" column="discountamount"/>
    	<property name="payCost" column="paycost"/>
    	<property name="payNumber" column="payNumber"/>
    	<property name="testDate" column="testDate"/>
    	<property name="licenseStatus" column="licensestatus"/>

		<property name="CertificateNum" column="CertificateNum"/>
		<property name="CertificateUsedDate" column="CertificateUsedDate"/>
		<property name="dueToDate" column="duetodate"/>
		<property name="studentId" column="studentId"/>
		<property name="FirstRetestFee" column="FirstRetestFee"/>
		<property name="SecondRetestFee" column="SecondRetestFee"/>
		<property name="ThirdRetestFee" column="ThirdRetestFee"/>
		<property name="FirstRetesPayNumber" column="FirstRetesPayNumber"/>
		<property name="SecondRetestPayNumber" column="SecondRetestPayNumber"/>
		<property name="ThirdRetesPayNumber" column="ThirdRetesPayNumber"/>

		<property name="status" column="status"/>
	</class>
</hibernate-mapping>